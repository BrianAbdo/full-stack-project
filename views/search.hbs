<div class="cover-container content-wrapper d-flex w-100 h-100 p-3 mx-auto flex-column">

  {{> navbar user=user}}


<div>
  <h2>Welcome {{user.username}}</h2>
  </div>

  <form id="searchForm" action="">
    <div class="row mb-4">
      <div class="form-group col-md-9">
        <input id="search-input" type="search" placeholder="Search For Your High School"
          class="form-control form-control-underlined">
      </div>
      <div class="form-group col-md-3">
        <button type="submit" id="searchButton"
          class="btn btn-primary rounded-pill btn-block shadow-sm float-right float-md-none">Search</button>
      </div>
    </div>
  </form>
  <div class="container">
<div id = "searchResults">

</div>
</div>
  {{> footer user=user}}
</div>

